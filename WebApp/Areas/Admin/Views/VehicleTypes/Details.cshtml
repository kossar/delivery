@using Base.Resources.WebApp
@model BLL.App.DTO.VehicleType

@{
    ViewData["Title"] = WebAppUiCommon.Details;
}

<h1>@WebAppUiCommon.Details</h1>

<div>
    <h4>@Resources.BLL.App.DTO.VehicleType._VehicleType</h4>
    <hr/>
    <partial name="_Details" model="Model"/>
</div>
<div>

    @if (User.IsInRole("Admin"))
    {
        <a asp-action="Edit" asp-route-id="@Model.Id">@WebAppUiCommon.Edit</a> <text>|</text>
    }

    <a asp-action="Index">@WebAppUiCommon.BackToList</a>
</div>